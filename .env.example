# .env file - DO NOT COMMIT TO GIT
# Copy this to .env and fill in your actual values

# ============================================
# SUPABASE (Required - Primary Data Store)
# ============================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_or_service_key

# ============================================
# GOOGLE DRIVE (Required)
# ============================================
GOOGLE_DRIVE_CREDENTIALS_FILE=credentials.json
GOOGLE_DRIVE_FOLDER_ID=your_folder_id_here
GOOGLE_DRIVE_PROCESSED_FOLDER_ID=your_processed_folder_id_here

# ============================================
# HUGGINGFACE (Optional - for speaker diarization)
# Get token from: https://huggingface.co/settings/tokens
# ============================================
HUGGINGFACE_TOKEN=hf_your_token_here

# ============================================
# PROCESSING SETTINGS
# ============================================
CHECK_INTERVAL_SECONDS=120
WHISPER_MODEL=large-v3
ENABLE_DIARIZATION=true
TEMP_AUDIO_DIR=./temp
LOG_DIR=./logs
TRANSCRIPTS_FOLDER=./Transcripts

# ============================================
# NOTION (Legacy - Optional)
# NOTE: This pipeline writes to Supabase.
# Notion sync is handled separately by jarvis-backend.
# ============================================
# NOTION_API_KEY=secret_your_notion_integration_key
# NOTION_MEETING_DATABASE_ID=your_database_id
# NOTION_CRM_DATABASE_ID=your_database_id
# NOTION_TASKS_DATABASE_ID=your_database_id
# NOTION_REFLECTIONS_DATABASE_ID=your_database_id
